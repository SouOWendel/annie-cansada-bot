{
	"name": "annie-cansada",
	"version": "2.0.1",
	"description": "Annie Cansada, uma personagem de Devilline HQ que virou um bot multifuncional.",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"start": "node index.js",
		"restart": "node deploy-commands.js && node index.js",
		"register": "node deploy-commands.js",
		"dev": "nodemon index.js",
		"commitlint": "commitlint --edit",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prepare": "husky install",
		"commit": "git-cz"
	},
	"author": "Wendel and Shay DeWael",
	"license": "MIT",
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.16.0",
		"@distube/spotify": "^1.5.1",
		"@distube/yt-dlp": "^1.1.3",
		"@distube/ytdl-core": "^4.11.17",
		"ascii-table3": "^0.8.2",
		"body-parser": "^1.20.2",
		"cpu-stat": "^2.0.1",
		"discord-api-types": "^0.37.39",
		"discord-interactions": "^3.2.0",
		"discord.js": "^14.9.0",
		"distube": "^4.0.6",
		"dotenv": "^16.0.3",
		"epic-free-games": "^4.0.1",
		"ffmpeg": "^0.0.4",
		"ffmpeg-static": "^5.1.0",
		"formidable": "^3.5.0",
		"fs": "^0.0.1-security",
		"libsodium-wrappers": "^0.7.11",
		"node-crc": "^1.3.2",
		"node-fetch": "^3.3.0",
		"observable-slim": "^0.1.6",
		"prism-media": "^2.0.0-alpha.0",
		"superagent": "^8.0.9",
		"tweetnacl": "^1.0.3",
		"yt-search": "^2.10.4"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-conventional": "^17.7.0",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin": "^1.0.1",
		"eslint-plugin-import-helpers": "^1.3.1",
		"husky": "^8.0.3",
		"nodemon": "^2.0.15"
	},
	"repository": "https://github.com/Mark57449/Annie-Cansada.git",
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
