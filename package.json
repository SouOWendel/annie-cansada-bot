{
	"name": "annie-cansada",
	"version": "2.1.0",
	"description": "Annie Cansada, uma personagem de Devilline HQ que virou um bot multifuncional.",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"start": "node index.js",
		"restart": "node deploy-commands.js && node index.js",
		"register": "node deploy-commands.js",
		"dev": "nodemon index.js",
		"commitlint": "commitlint --edit",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"prepare": "husky install",
		"commit": "git-cz"
	},
	"author": "Wendel Henrique (@souowendel)",
	"license": "MIT",
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.17.0",
		"@distube/spotify": "^2.0.2",
		"@distube/youtube": "^1.0.2",
		"@distube/yt-dlp": "^2.0.1",
		"@distube/ytdl-core": "^4.13.4",
		"ascii-table3": "^0.9.0",
		"body-parser": "^1.20.2",
		"cpu-stat": "^2.0.1",
		"discord-api-types": "^0.37.92",
		"discord-interactions": "^4.0.0",
		"discord.js": "^14.15.3",
		"distube": "^5.0.2",
		"dotenv": "^16.4.5",
		"epic-free-games": "^4.0.2",
		"ffmpeg": "^0.0.4",
		"ffmpeg-static": "^5.2.0",
		"formidable": "^3.5.1",
		"fs": "^0.0.1-security",
		"libsodium-wrappers": "^0.7.13",
		"observable-slim": "^0.1.6",
		"prism-media": "^2.0.0-alpha.0",
		"tweetnacl": "^1.0.3",
		"yt-search": "^2.11.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin": "^1.0.1",
		"husky": "^9.0.11",
		"nodemon": "^3.1.4"
	},
	"repository": "https://github.com/Mark57449/Annie-Cansada.git",
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
			"prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
		}
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
